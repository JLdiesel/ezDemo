{
  "compilerOptions": {
    "target": "es2016" /* 目标语法 */,
    "module": "esnext" /* 模块格式 */,
    "moduleResolution": "node" /*模块解析方式 */,
    "outDir": "dest" /* 输出目录 */,
    "jsx": "preserve", //不转译jsx
    "lib": ["ESNext", "DOM"], //支持的类库
    "baseUrl": ".",
    "paths": {
      "@vue/*": ["packages/*/src"], //alias 快捷查找path
      "@test/*": ["packages/*"] //alias 快捷查找path
    }
  }
}
